{
    "name": "swicku/anonymization-bundle",
    "description": "Symfony anonymization bundle providing extendable anonymization and compliance with newest php versions",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.0.18",
        "symfony/http-kernel": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/property-access": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Swicku\\AnonymizationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swicku\\AnonymizationBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1"
    }
}
